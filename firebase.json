{
    "hosting":[
        {
            "target":"sepr-docs",
            "public":"build",
            "ignore":[
                "firebase.json",
                "**/.*",
                "**/node_modules/**"
            ],
            "rewrites":[
                {
                    "source":"**",
                    "destination":"/index.html"
                }
            ]
        },
        {
            "target":"javadocs",
            "public":"javadocs"
        }
    ]
}
